{
  "name": "contribute-page-v2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "react-stripe-elements": "^2.0.3"
  },
  "scripts": {
    "start": "npm run sass:build && react-scripts start",
    "build": "npm run sass:build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sass": "sass --watch src/styles/scss:src/styles/css",
    "sass:watch": "node-sass -w src/styles/scss -o src/styles/css",
    "sass:build": "node-sass src/styles/scss -o src/styles/css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass": "^4.11.0"
  }
}
